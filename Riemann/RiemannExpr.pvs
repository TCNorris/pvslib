RiemannExpr : DATATYPE
  BEGIN

  CONST(x: rat): const?
  ADD(e1, e2: RiemannExpr): add?
  EXP: exp?
  MULT(e1, e2: RiemannExpr): mult?
  NEG(e: RiemannExpr): neg?
  POW(e1: RiemannExpr, n: nat): pow?
  SUB(e1, e2: RiemannExpr): sub?
  SIN: sin?
  COS: cos?
%  DIV(e1, e2: RiemannExpr): div?
%  SQRT(e: RiemannExpr): sqrt?
%  LN(e: RiemannExpr): ln?
  ID: id?
  COMP(e1, e2: RiemannExpr): comp?

  END RiemannExpr
